# Hash bang using the env command
snippet #
#! /usr/bin/env ${1:bash}
endsnippet

snippet mode "No eol colon version"
${1:#} ex: ${2:fdm&} ${3:sw=4}
endsnippet

snippet mode "eol colon version"
${1:#} ex: se ${2:fdm&} ${3:sw=4}:
endsnippet

